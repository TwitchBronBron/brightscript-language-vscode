{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2018",
        "noImplicitAny": false,
        "removeComments": false,
        "noUnusedLocals": false,
        "noImplicitThis": true,
        "inlineSourceMap": false,
        "sourceMap": true,
        "outDir": "../out",
        "preserveConstEnums": true,
        "strictNullChecks": false,
        "noUnusedParameters": false
    }
}